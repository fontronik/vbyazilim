<%
out = []
blog.articles.each_with_index do |article, index|
    title = "#{article.metadata[:page][:category]} : #{article.title}" 
    title = "#{title} [#{article.tags.join(', ')}]" if article.tags.count > 0
    out << {
        title: title,
        url: article.url,
    }
end
concat out.to_json
%>
